{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "esnext",
    "rootDir": "./src",
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": false,
    "inlineSources": true,
    "sourceMap": true,
    "noEmitOnError": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "baseUrl": "./src",
    "strict": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}
